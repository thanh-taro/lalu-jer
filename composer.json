    "license": "MIT",
    "authors": [{
        "name": "Nguyen Tri Thanh",
        "email": "thanhnt1@greenglobal.vn"
    }],
    "minimum-stability": "stable",
    "require": {
        "php": "~5.5|~7.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "art4/json-api-client": "*",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
